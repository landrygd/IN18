{
  "name": "in18",
  "main": "electron/index.js",
  "version": "1.2.0",
  "author": "Landry and Deackor",
  "homepage": "https://deakcor.itch.io/in18",
  "scripts": {
    "serve": "ng serve",
    "build": "ionic build --prod --release --minifyjs --minifycss --optimizejs",
    "prod": "ng build -prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "electron .",
    "electron": "ionic cap copy && electron .",
    "electron:win_debug": "electron-packager ./electron IN18 --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=debug --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='IN18' --icon=./electron/icon/favicon_invert.ico",
    "electron:win": "electron-packager ./electron IN18 --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='IN18' --icon=./electron/icon/favicon_invert.ico",
    "electron:linux": "electron-packager ./electron IN18 --overwrite --asar=true --platform=linux --arch=x64 --icon=./electron/icon/favicon_invert.png --prune=true --out=release-builds",
    "electron:mac": "electron-packager ./electron IN18 --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --icon=./electron/icon/favicon_invert.icns"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^10.0.8",
    "@angular/core": "^10.0.8",
    "@angular/forms": "^10.0.8",
    "@angular/platform-browser": "^10.0.8",
    "@angular/platform-browser-dynamic": "^10.0.8",
    "@angular/router": "^10.0.8",
    "@angular/service-worker": "^10.0.8",
    "@capacitor-community/electron": "^1.2.3",
    "@capacitor/core": "^2.4.0",
    "@capacitor/electron": "^2.4.0",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/file": "^5.27.0",
    "@ionic-native/file-transfer": "^5.27.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.3.1",
    "capacitor": "^0.5.5",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-file-transfer": "^1.7.1",
    "deepl": "^1.0.13",
    "electron": "^11.5.0",
    "electron-is-dev": "^1.2.0",
    "file-saver": "^1.3.8",
    "install": "^0.13.0",
    "jszip": "^3.5.0",
    "ngx-dropzone": "^2.2.2",
    "ngx-electron": "^2.2.0",
    "npm": "^6.14.7",
    "rxjs": "~6.5.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular/cli": "10.2.1",
    "@angular/compiler": "^10.0.8",
    "@angular/compiler-cli": "^10.0.8",
    "@angular/language-service": "^10.0.8",
    "@capacitor/cli": "^2.4.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "codelyzer": "^6.0.0",
    "electron": "^11.5.0",
    "electron-bridge": "^0.1.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-packager": "^15.2.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "description": "A i18n translator",
  "cordova": {
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-file-transfer": {}
    }
  }
}
